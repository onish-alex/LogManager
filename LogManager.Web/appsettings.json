{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RequestSettings": {
    "DefaultUrl": "http://tariscope.com",
    "AllowableExtensions": [ ".html", ".htm", ".php" ],
    "HttpMethods": [ "POST", "GET", "HEAD" ]
  },
  "ConnectionSettings": {
    "ConnectionString": "Data Source=.\\SQLEXPRESS;Initial Catalog=LogManager;Integrated Security=true"
  },
  "FileSettings": {
    "PathToSave": "D:/Documents/"
  },
  "PageSettings": {
    "LinkCount": 20
  }
}
